//Lab 1
#include <iostream>
#include <limits>
using namespace std;

int main() {
    int a = 1;
    int b = 2;
    int c = 3;

    cout << "Input: " << a << b << c << endl;
    cout << "a+b*c = " << a + b * c << endl;
    cout << "(a+b)*c = " << (a+b) * c << endl;
    cout << "a/b*c = " << a/b * c << endl;
    cout << "a/(b*c) = " << a/(b * c) << endl;

    return 0;
}

//Lab 2

